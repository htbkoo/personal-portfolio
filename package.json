{
  "name": "hey-personal-portfolio",
  "version": "0.10.0",
  "private": true,
  "homepage": "https://htbkoo.github.io/personal-portfolio",
  "scripts": {
    "lint": "next lint",
    "dev": "cross-env NEXT_PUBLIC_GIT_SHA=`git rev-parse --short HEAD` BROWSER=none next dev",
    "start": "cross-env NEXT_PUBLIC_GIT_SHA=`git rev-parse --short HEAD` BROWSER=none next start",
    "build": "cross-env NEXT_PUBLIC_GIT_SHA=`git rev-parse --short HEAD` next build && next export",
    "dev-win": "git rev-parse --short HEAD >sha.txt && set /p NEXT_PUBLIC_GIT_SHA= < sha.txt && del sha.txt && set BROWSER=none && next dev",
    "start-win": "git rev-parse --short HEAD >sha.txt && set /p NEXT_PUBLIC_GIT_SHA= < sha.txt && del sha.txt && set BROWSER=none && next start",
    "build-win": "git rev-parse --short HEAD >sha.txt && set /p NEXT_PUBLIC_GIT_SHA= < sha.txt && del sha.txt && next build && next export",
    "test": "jest --ci",
    "test:watch": "jest --ci",
    "gh-pages:rebuild": "yarn run gh-pages:clean && yarn run build && yarn run gh-pages:rename",
    "gh-pages:clean": "rimraf docs",
    "gh-pages:rename": "node ./bin/rename -s out -d docs",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@fontsource/roboto": "4.4.0",
    "@material-ui/core": "4.12.3",
    "@material-ui/icons": "4.11.2",
    "@material-ui/styles": "4.11.4",
    "cheerio": "1.0.0-rc.10",
    "next": "12.1.5",
    "promise-polyfill": "^8.1.0",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-ga": "3.3.0",
    "react-plx": "^1.3.11",
    "react-scrollspy": "^3.3.5",
    "rss-parser": "3.6.2",
    "ts-react-codepen-embed": "^0.3.3",
    "url-polyfill": "^1.1.5"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.1.21",
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addon-links": "^6.1.21",
    "@storybook/node-logger": "^6.1.21",
    "@storybook/react": "^6.1.21",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.0.1",
    "@testing-library/user-event": "14.1.0",
    "@types/cheerio": "0.22.30",
    "@types/jest": "26.0.20",
    "@types/node": "17.0.23",
    "@types/react": "17.0.44",
    "@types/react-dom": "17.0.15",
    "args": "^5.0.0",
    "babel-jest": "27.5.1",
    "cross-env": "^5.2.0",
    "eslint": "8.13.0",
    "eslint-config-next": "12.1.5",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-testing-library": "5.3.1",
    "image-trace-loader": "1.0.2",
    "imagemin-gifsicle": "7.0.0",
    "imagemin-mozjpeg": "9.0.0",
    "imagemin-optipng": "8.0.0",
    "imagemin-svgo": "9.0.0",
    "jest": "27.5.1",
    "jest-when": "^2.2.0",
    "jimp": "0.16.1",
    "lqip-loader": "2.2.1",
    "mv": "^2.1.1",
    "next-compose-plugins": "2.2.1",
    "next-optimized-images": "2.6.2",
    "next-pwa": "5.5.0",
    "prettier": "^2.2.1",
    "prop-types": "15.7.2",
    "responsive-loader": "2.3.0",
    "rimraf": "^2.6.3",
    "storybook-addon-next": "^1.6.2",
    "svg-sprite-loader": "6.0.11",
    "typescript": "4.6.3",
    "webp-loader": "0.6.0"
  },
  "resolutions": {
    "@types/react": "17.0.44",
    "@types/react-dom": "17.0.15"
  },
  "license": "MIT"
}
