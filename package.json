{
  "name": "hey-personal-portfolio",
  "version": "0.10.0",
  "private": true,
  "homepage": "https://htbkoo.github.io/personal-portfolio",
  "scripts": {
    "lint": "next lint",
    "dev": "cross-env REACT_APP_GIT_SHA=`git rev-parse --short HEAD` BROWSER=none next dev",
    "start": "cross-env REACT_APP_GIT_SHA=`git rev-parse --short HEAD` BROWSER=none next start",
    "build": "cross-env REACT_APP_GIT_SHA=`git rev-parse --short HEAD` next build",
    "dev-win": "git rev-parse --short HEAD >sha.txt && set /p REACT_APP_GIT_SHA= < sha.txt && del sha.txt && set BROWSER=none && next dev",
    "start-win": "git rev-parse --short HEAD >sha.txt && set /p REACT_APP_GIT_SHA= < sha.txt && del sha.txt && set BROWSER=none && next start",
    "build-win": "git rev-parse --short HEAD >sha.txt && set /p REACT_APP_GIT_SHA= < sha.txt && del sha.txt && next build",
    "test": "jest --ci",
    "test:watch": "jest --ci",
    "gh-pages:rebuild": "yarn run gh-pages:clean && yarn run build && yarn run gh-pages:rename",
    "gh-pages:clean": "rimraf docs",
    "gh-pages:rename": "node ./bin/rename -s build -d docs",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "plugins": [
      "only-warn"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@fontsource/roboto": "4.4.0",
    "@material-ui/core": "4.12.3",
    "@material-ui/icons": "4.11.2",
    "@material-ui/styles": "4.11.4",
    "cheerio": "1.0.0-rc.10",
    "promise-polyfill": "^8.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-ga": "3.3.0",
    "react-plx": "^1.3.11",
    "react-scrollspy": "^3.3.5",
    "rss-parser": "3.6.2",
    "ts-react-codepen-embed": "^0.3.3",
    "url-polyfill": "^1.1.5"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.1.21",
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addon-links": "^6.1.21",
    "@storybook/node-logger": "^6.1.21",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.1.21",
    "@types/cheerio": "0.22.30",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.20",
    "@types/react": "17.0.43",
    "@types/react-dom": "17.0.14",
    "args": "^5.0.0",
    "cross-env": "^5.2.0",
    "eslint-plugin-only-warn": "^1.0.3",
    "jest-when": "^2.2.0",
    "mv": "^2.1.1",
    "prettier": "^2.2.1",
    "prop-types": "15.7.2",
    "react-scripts": "4.0.3",
    "rimraf": "^2.6.3",
    "typescript": "4.6.3"
  },
  "license": "MIT"
}
